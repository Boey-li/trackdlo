<launch>

  <!-- (start your tracking system's ROS driver) -->
  <arg name="user" default="ablcts18" />
  <include file="$(find realsense2_camera)/launch/rs_camera.launch">
    <arg name="json_file_path" value="/home/$(arg user)/Desktop/preset_decimation_4.0_depth_step_100.json" />
    <arg name="publish_tf" value="false" />
    <arg name="filters" value="pointcloud,temporal,decimation" />
    <arg name="depth_fps" value="6" />
    <arg name="depth_height" value="720" />
    <arg name="depth_width" value="1280" />
    <arg name="color_fps" value="6" />
    <arg name="color_height" value="720" />
    <arg name="color_width" value="1280" />
    <arg name="align_depth" value="true" />
    <arg name="ordered_pc" value="true" />
  </include>

  <!-- include file="$(find apriltag_ros)/launch/continuous_detection.launch" -->
  <!-- include file="$(find charuco_detector)/launch/charuco_detector.launch" -->
  <!-- <include file="$(find test)/launch/python_charuco_detector.launch" /> -->

  <!-- <arg name="namespace_prefix" default="irb120_eih_calib" />
  <arg name="eye_on_hand" default="true" />
  <arg if="$(arg eye_on_hand)" name="namespace" value="$(arg namespace_prefix)_eye_on_hand" />
  <arg unless="$(arg eye_on_hand)" name="namespace" value="$(arg namespace_prefix)_eye_on_base" /> -->

  <!-- publish the hand-eye calibration computed with easy_handeye -->
  <!-- <include file="$(find easy_handeye)/launch/publish.launch" >
    <arg name="eye_on_hand" value="true" />
    <arg name="namespace_prefix" value="$(arg namespace_prefix)" />
  </include> -->

  <node name="realsense_tf" pkg="test" type="pub_camera_tf.py"/>
  <node name="opencv_interaction" pkg="test" type="opencv_interactive.py"/>
  <node name="tracking_eval_existing_method" pkg="test" type="eval_mct_predict.py"/>

  <!-- tracking rviz file -->
  <node type="rviz" name="rviz" pkg="rviz" args="-d $(find test)/rviz/tracking_eval.rviz" />

  <!-- start Rviz with our configuration, in the move group's namespace 
  <node if="$(arg start_rviz)" ns="$(arg move_group_namespace)" name="$(anon rviz)" pkg="rviz" type="rviz" respawn="true"
        args="-d $(arg rviz_config_file)" output="screen" />
  -->

<!--   <include file="$(find abb_irb120_moveit_config)/launch/moveit_rviz.launch">
    <arg name="config" value="true"/>
    <arg name="debug" value="false"/>
  </include> -->

</launch>